<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="571.0" prefWidth="894.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csc325_firebase_webview_auth.view.LoginController">
   <ImageView fx:id="splashImage" fitHeight="571.0" fitWidth="894.0">
      <image>
         <Image url="@/files/Login.jpg" />
      </image>
   </ImageView>
   <VBox layoutX="314.0" prefHeight="571.0" prefWidth="273.0">
      <children>
         <VBox prefHeight="200.0" prefWidth="100.0" />
         <TextField fx:id="emaillTextField" promptText="Email" />
         <PasswordField fx:id="passwordField" promptText="Password" />
         <HBox prefHeight="42.0" prefWidth="273.0">
            <children>
               <Button fx:id="loginButton" mnemonicParsing="false" onAction="#handleLogin" prefHeight="25.0" prefWidth="140.0" text="Login" />
               <VBox prefHeight="42.0" prefWidth="22.0" />
               <Button fx:id="signUpButton" mnemonicParsing="false" onAction="#handleSignUp" prefHeight="25.0" prefWidth="127.0" text="Sign Up" />
            </children>
         </HBox>
      </children></VBox>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
