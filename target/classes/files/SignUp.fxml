<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="571.0" prefWidth="894.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csc325_firebase_webview_auth.view.SignUpController">
   <ImageView fx:id="splashImage" fitHeight="571.0" fitWidth="894.0">
      <image>
         <Image url="@Login.jpg" />
      </image>
   </ImageView>
   <VBox layoutX="314.0" prefHeight="571.0" prefWidth="273.0">
      <children>
         <VBox prefHeight="200.0" prefWidth="100.0" />
         <TextField fx:id="emailTextField" promptText="Email" />
         <PasswordField fx:id="passwordField" promptText="Password" />
         <PasswordField fx:id="confirmPasswordField" promptText="Confirm Password" />
         <HBox prefHeight="37.0" prefWidth="273.0">
            <children>
               <HBox prefHeight="37.0" prefWidth="281.0">
                  <children>
                     <Button fx:id="registerButton" mnemonicParsing="false" onAction="#handleSignUp" prefHeight="25.0" prefWidth="109.0" text="Register" />
                     <HBox prefHeight="37.0" prefWidth="67.0" />
                     <Button fx:id="backToLoginButton" mnemonicParsing="false" onAction="#handleBackToLogin" prefHeight="25.0" prefWidth="100.0" text="Back to Login" />
                  </children>
               </HBox>
            </children>
         </HBox>
      </children></VBox>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
